{
  "name": "Mikan",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "__MSG_extDescription__",
  "homepage_url": "http://mikanani.me",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "zh_CN",
  "background": {
    "scripts": [
      "bower_components/jquery/dist/jquery.js",
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "options_page": "src/options_custom/index.html",
    "permissions": [
        //"clipboardWrite",
        "notifications",
        "background",
        "tabs",
        "http://*.mikanani.me/",
        "http://mikanani.me/"
    ],
  "omnibox": {
    "keyword": "mikan"
  }
}