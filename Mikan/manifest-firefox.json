{
    "gecko": {
      "id": "mikan@mikanani.me"
    },
    "background": {
        "scripts": [
            "bower_components/jquery/dist/jquery.js",
            "src/bg/background.js"
        ],
        "persistent": true
    },
    "content_scripts": [
        {
            "matches": [ "http://mikanani.me/*" ],
            "js": [
                "bower_components/jquery/dist/jquery.js",
                "src/inject/inject.js"
            ]
        }
    ],
    "default_locale": "zh_CN",
    "description": "__MSG_extDescription__",
    "homepage_url": "http://mikanani.me",
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "manifest_version": 2,
    "name": "Mikan",
    "permissions": [
        //"clipboardWrite",
        "notifications",
        "background",
        "tabs",
        "http://*.mikanani.me/",
        "http://mikanani.me/"
    ],
    "version": "0.0.1"
}