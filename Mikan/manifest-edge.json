{
    "author": "MikanProject",
    "background": {
        "page": "src/edgelib/background.html",
        "persistent": true
    },
    "browser_action": {
        "default_icon": {
            "40": "icons/icon40.png"
        },
        "default_title": "Mikan",
        "default_popup": "src/index.html"
    },
    "content_scripts": [
        {
            "matches": [
                "http://mikanani.me/*"
            ],
            "js": [
                "lib/jquery/jquery.js",
                "src/inject/inject.js"
            ]
        }
    ],
    "default_locale": "zh_CN",
    "description": "__MSG_extDescription__",
    "homepage_url": "http://mikanani.me",
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "manifest_version": 2,
    "name": "Mikan",
    "permissions": [
        "tabs",
        "http://*.mikanani.me/",
        "http://mikanani.me/",
        "webRequest",
        "webRequestBlocking",
        "cookies",
        "http://dynamic.cloud.vip.xunlei.com/"
    ],
    "version": "0.0.5.4",
    "-ms-preload": {
        "backgroundScript": "src/edgelib/backgroundScriptsAPIBridge.js",
        "contentScript": "src/edgelib/contentScriptsAPIBridge.js"
    }
}