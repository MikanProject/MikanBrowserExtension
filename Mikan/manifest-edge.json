{
    "author": "MikanProject",
    "background": {
        "page": "edgelib/background.html",
        "persistent": true
    },
    "browser_action": {
        "default_icon": {
            "40": "icons/icon40.png"
        },
        "default_title": "Mikan",
        "default_popup": "index.html"
    },
    "content_scripts": [
        {
            "matches": [
                "http://mikanani.me/*",
                "https://mikanani.me/*"
            ],
            "js": [
                "lib/jquery/jquery.js",
                "inject/inject.js"
            ]
        }
    ],
    "default_locale": "zh_CN",
    "description": "__MSG_extDescription__",
    "homepage_url": "https://mikanani.me",
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "manifest_version": 2,
    "name": "Mikan",
    "permissions": [
        "tabs",
        "http://*.mikanani.me/",
        "http://mikanani.me/",
        "https://*.mikanani.me/",
        "https://mikanani.me/"
    ],
    "version": "0.0.6.3",
    "-ms-preload": {
        "backgroundScript": "edgelib/backgroundScriptsAPIBridge.js",
        "contentScript": "edgelib/contentScriptsAPIBridge.js"
    }
}